Sub SessionProc_Run(c as Context)
	
	'''''''''''''''''''''''''''''''''''''''''''''
	' InLine - Initialization BEGIN
	'''''''''''''''''''''''''''''''''''''''''''''
	perf = 0

TotalTrial = 0

